    <div id="wrapper">
        <div class="main-container">
            <div class="container">
                <div class="row">
                    <div class="col-sm-3 page-sidebar">
                        <aside>
                            <div class="inner-box">
                                <div class="user-panel-sidebar">
                                    <div class="collapse-box">
                                       <h5 class="collapse-title no-border"> My Classified <a href="#MyClassified" class="fa fa-angle-down"></i></a></h5>
                                                                      

          <div class="panel-collapse collapse in" id="MyClassified">
                                            <ul class="acc-list">
                                                <li> <a class="active" href="/posts/account_home"><i class="icon-home"></i>
                                                    Personal Home </a></li> 

                                            </ul>
                                        </div>
                                    </div>
                                    <!-- /.collapse-box  -->
                                    <div class="collapse-box">
                                        <h5 class="collapse-title"> My Ads <a href="#MyAds" data-toggle="collapse"
                                                                              class="pull-right"><i
                                                class="fa fa-angle-down"></i></a></h5>

                                        <div class="panel-collapse collapse in" id="MyAds">
                                            <ul class="acc-list">
                                                <li><a href="/userpanel/history"><i class="icon-docs"></i> History <span
                                                        class="badge">42</span> </a></li>
                                                <li><a href="/userpanel/prescription"><i class="icon-heart"></i>
                                                    Prescription <span class="badge">42</span> </a></li>
                                                <li><a href="/userpanel/video"><i class="icon-star-circled"></i>
                                                    Video calling/ chat <span class="badge">42</span> </a></li>
                                                
                                            </ul>
                                        </div>
                                    </div>
                                    <!-- /.collapse-box  -->

                                    <div class="collapse-box">
                                        <h5 class="collapse-title"> <a href="#TerminateAccount"  data-toggle="collapse" class="pull-right"><i class="fa fa-angle-down"></i></a></h5>

                                        <div class="panel-collapse collapse in" id="TerminateAccount">
                                            <ul class="acc-list">
                                                <li><!-- <a href="/posts/close_account"><i class="icon-cancel-circled "></i> Delete
                                                    account </a> -->
                                                <%= button_to "Delete my account", registration_path(resource_name), data: { confirm: "Are you sure?" }, method: :delete %></li>

                                            </ul>
                                        </div>
                                    </div>
                                    <!-- /.collapse-box  -->
                                </div>
                            </div>
                            <!-- /.inner-box  -->

                        </aside>
                    </div>
                    <!--/.page-sidebar-->

                    <div class="col-sm-9 page-content">
                        <div class="inner-box">
                            <div class="row">
                                <div class="col-md-5 col-xs-4 col-xxs-12">
                                    <h3 class="no-padding text-center-480 useradmin"><a href="/"><%= image_tag("user.jpg",class: "userImg",alt: "user") %> <%= current_user.firstname%>
                                    </a></h3>
                                </div>
                                                            </div>
                        </div>

                        <div class="inner-box">
                            <div class="welcome-msg">
                                <h3 class="page-sub-header2 clearfix no-padding">Hello <%= current_user.firstname%> </h3>
                                <span class="page-sub-header-sub small">You last logged in at: <%=current_user.last_sign_in_at%></span>
                            </div>
                            <div id="accordion" class="panel-group">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h4 class="panel-title"><a href="#collapseB1" data-toggle="collapse"> My
                                            details </a></h4>
                                    </div>
                                    <div class="panel-collapse collapse in" id="collapseB1">
                                        <div class="panel-body">
                                            <%= resource_name.to_s.humanize %>
                         <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %> 
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">First Name</label>

                                                    <div class="col-sm-9">
                                                      <%#= f.text_field :firstname, autofocus: true,:class=>"form-control"%>
                                                        <input type="text" class="form-control" name="user[firstname]"
                                                        value="<%= current_user.firstname %>" placeholder="<%= current_user.firstname %>">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">Last name</label>

                                                    <div class="col-sm-9">
                                                        <input type="text" class="form-control" name="user[lastname]"
                                                        value="<%= current_user.lastname %>" placeholder="<%= current_user.lastname %>">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">Email</label>

                                                    <div class="col-sm-9">
                                                       <%#= f.email_field :email, autofocus: true,:class=>"form-control" %>
                                                       <input type="email" class="form-control" name="user[email]"
                                                        value="<%= current_user.email %>" placeholder="<%= current_user.email %>">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">password</label>

                                                    <div class="col-sm-9">
                                                        <input type="password" class="form-control" name="user[password]"
                                                        value="<%= current_user.password %>" placeholder="<%= current_user.password %>">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="Phone" class="col-sm-3 control-label">Confirm Password</label>

                                                    <div class="col-sm-9">
                                                        <input type="password" class="form-control" name="user[password_confirmation]"
                                                        value="<%= current_user.password_confirmation %>" placeholder="<%= current_user.password_confirmation %>">

                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">Current Password</label>

                                                    <div class="col-sm-9">
                                                        <input type="password" class="form-control" name="user[current_password]"
                                                        value="<%= current_user.current_password %>" placeholder="<%= current_user.current_password %>">
                                                    </div>
                                                </div>

                                                <div class="form-group hide"> <!-- remove it if dont need this part -->
                                                    <label class="col-sm-3 control-label">Facebook account map</label>

                                                    <div class="col-sm-9">
                                                        <div class="form-control"><a class="link" href="fb.com">Jhone.doe</a> <a class=""> <i class="fa fa-minus-circle"></i></a></div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <div class="col-sm-offset-3 col-sm-9"></div>
                                                </div>
                                                <div class="form-group">
                                                    <div class="col-sm-offset-3 col-sm-9">
                                                        <button type="submit" class="btn btn-default">Update</button>
                                                    </div>
                                                </div>
                                            <% end %>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                            <!--/.row-box End-->

                        </div>
                    </div>
                    <!--/.page-content-->
                </div>
                <!--/.row-->
            </div>
            <!--/.container-->
        </div>
        <!-- /.main-container -->
        </div>
    <!-- /.wrapper --> <!-- Le javascript
    ================================================== -->

