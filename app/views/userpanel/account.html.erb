<div class="container">
<h1>My Account</h1>
<%# if current_@user.super_admin? %>
      <%#= link_to 'Add New @user/admin', register_path %>
    <%# end %>
<div id="table" class="table-editable">
    <table class="table">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Gender</th>
        <th>City</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Online Hours</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
      
        <tr>
        <td contenteditable="false"><%= @user.firstname%></td>
        <td contenteditable="false"><%= @user.lastname%></td>
        <td contenteditable="false"><%= @user.gender%></td>
        <td contenteditable="false"><%= @user.city%></td>
        <td contenteditable="false"><%= @user.phone%></td>
        <td contenteditable="false"><%= @user.email%></td>
        <td contenteditable="false"><%= Time.at((current_user.total_online_time) * 60).utc.strftime("%H:%M:%S")%></td>

       
        
        <td contenteditable="false"><%#= link_to "Edit", edit_user_path(@user)%></td>
        <td contenteditable="false"><%#= link_to "Delete", @user, method: :delete, data: { confirm: 'Are you sure?' }%></td>
      </tr>

        
        
      <!-- This is our clonable table line -->
      
    </table>
  </div>

</div>